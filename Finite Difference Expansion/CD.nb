    RowBox[{"[", 
     RowBox[{"n", "+", "1"}], "]"}], "]"}], "/", 
   RowBox[{
    SuperscriptBox["f", 
     TagBox[
      RowBox[{"(", "n", ")"}],
      Derivative],
     MultilineFunction->None], "[", "x0", "]"}], " "}]}]}], "Code",
